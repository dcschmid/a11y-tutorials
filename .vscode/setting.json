{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "astro",
        "typescript",
        "typescriptreact"
    ],
    "prettier.documentSelectors": ["**/*.astro"],
    "[astro]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.autoSave": "afterDelay",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true
}
